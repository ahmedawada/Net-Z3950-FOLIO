{
  "okapi": {
    "url": "https://uchicago-test-okapi.folio.indexdata.com",
    "tenant": "uchicago"
  },
  "login": {
    "username": "${OKAPI_USER-uchicago_admin}",
    "password": "${OKAPI_CHICAGO_PASSWORD}"
  },
  "indexMap": {
    "7": "identifiers/@value/@identifierTypeId=\"cf0a2485-22e9-5be4-ac5c-fe393fb964e4\""
  },
  "postProcessing": {
    "marc": {
      "005": [
        { "op": "regsub", "pattern": "[123]", "replacement": "X/Y", "flags": "g" }
      ],
      "008": { "op": "regsub", "pattern": "([13579])", "replacement": "[$1]", "flags": "g" },
      "245$a": [
        { "op": "stripDiacritics" },
        { "op": "regsub", "pattern": "[abc]", "replacement": "*", "flags": "g" }
      ]
    }
  },
  "chunkSize": 1
}
